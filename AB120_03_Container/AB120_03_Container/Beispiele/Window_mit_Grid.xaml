<Window x:Class="AB120_03_Container.Beispiele.Window_mit_Grid"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AB120_03_Container"
        mc:Ignorable="d"
        Title="Window_mit_Grid" Height="120" Width="250">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition Width="100"/>
        </Grid.ColumnDefinitions>
        <Button Grid.Column="0" Grid.Row="0" Content="Button1"/>
        <Button Grid.Column="1" Grid.Row="0" Content="Button2"/>
        <Button Grid.Column="0" Grid.Row="1" Content="Button3"/>
        <Button Grid.Column="1" Grid.Row="1" Content="Button4"/>
    </Grid>
</Window>
